{
  "$id": "https://example.com/person.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Person",
  "type": "object",
  "properties": {
    "name": {
      "type": "object",
      "properties": {
        "first": {
          "type": "string"
        },
        "last": {
          "type": "string"
        }
      }
    },
    "age": {
      "type": "integer"
    },
    "ratio": {
      "type": "number"
    },
    "consent": {
      "type": "boolean"
    },
    "newsletter": {
      "type": "boolean"
    }
  }
}
